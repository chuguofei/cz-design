{
  "name": "cz-assistant",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "init": "pnpm run build:script && pnpm run icon:gen",
    "icon:gen": "pnpm --filter \"@cz-design/web\" run icon:gen",
    "build:script": "pnpm --filter \"@cz-design/scripts\" run build",
    "init:start": "pnpm run init && pnpm run start",
    "start": "pnpm --filter @cz-design/web run start",
    "build": "pnpm --filter @cz-design/web run build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@cz-design/web": "^1.0.0",
    "@cz-design/scripts": "^1.0.0",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "chalk": "^4.1.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.21.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "lerna": "^6.1.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.8.3"
  }
}
